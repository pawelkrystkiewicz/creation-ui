{
  "version": "0.1.0",
  "name": "creation-ui",
  "private": true,
  "main": "build/cjs/index.js",
  "module": "build/esm/index.js",
  "types": "build/index.d.ts",
  "styles": "build/index.css",
  "scripts": {
    "build:site": "tsc && vite build",
    "preview": "vite preview",
    "build-depr": "yarn tsc && NODE_ENV=production node --max-old-space-size=8192 node_modules/rollup/dist/bin/rollup -c && yarn build:clean-up",
    "build": "yarn tsc && yarn build:packages",
    "build:packages": "npx lerna run build",
    "start:prod": "next start",
    "sb": "start-storybook dev -p 6006",
    "build:sb": "storybook build",
    "amend": "git commit --amend --no-edit",
    "git:last-hash": "node ./scripts/get-last-git-hash.mjs",
    "nuke": "rm -rf node_modules yarn.lock && yarn cache clean --all && yarn",
    "prettify": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "_publish": "zsh ./scripts/shell/publish-all.sh && postpublish",
    "copy": "zsh ./scripts/shell/copy-file-to-each-subdir.sh",
    "git:force-update": "git push -f origin",
    "script:update": "node ./scripts/package-update-script.mjs",
    "next-version": "lerna version --conventional-commits",
    "postpublish": "zsh scripts/shell/remove-build.sh",
    "docs": "cd packages/docs && yarn start",
    "docs:build": "cd packages/docs && yarn build"
  },
